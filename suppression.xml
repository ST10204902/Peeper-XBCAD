<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <suppress>
        <notes>Known issue with ansi-regex, fixed in newer React Native versions</notes>
        <packageUrl regex="true">^pkg:npm/ansi-regex@.*$</packageUrl>
        <vulnerabilityName>CVE-2021-3807</vulnerabilityName>
    </suppress>
    <suppress>
        <notes>False positive in cross-spawn</notes>
        <packageUrl regex="true">^pkg:npm/cross-spawn@.*$</packageUrl>
        <cve>CVE-2020-8203</cve>
    </suppress>
    <suppress>
        <notes>Known issue in graphql, acceptable risk</notes>
        <packageUrl regex="true">^pkg:npm/graphql@.*$</packageUrl>
        <cve>CVE-2021-32644</cve>
    </suppress>
    <suppress>
        <notes>Legacy dependency from React Native</notes>
        <packageUrl regex="true">^pkg:npm/ini@.*$</packageUrl>
        <cve>CVE-2020-7788</cve>
    </suppress>
    <suppress>
        <notes>Dependency of Expo</notes>
        <packageUrl regex="true">^pkg:npm/kind-of@.*$</packageUrl>
        <cve>CVE-2019-20149</cve>
    </suppress>
    <suppress>
        <notes>Build tool dependency</notes>
        <packageUrl regex="true">^pkg:npm/minimatch@.*$</packageUrl>
        <cve>CVE-2022-3517</cve>
    </suppress>
    <suppress>
        <notes>iOS build tool dependency</notes>
        <packageUrl regex="true">^pkg:npm/plist@.*$</packageUrl>
        <cve>CVE-2022-24948</cve>
    </suppress>
    <suppress>
        <notes>Development dependency</notes>
        <packageUrl regex="true">^pkg:npm/qs@.*$</packageUrl>
        <cve>CVE-2022-24999</cve>
    </suppress>
    <suppress>
        <notes>Development server dependency</notes>
        <packageUrl regex="true">^pkg:npm/send@.*$</packageUrl>
        <cve>CVE-2022-24999</cve>
    </suppress>
    <suppress>
        <notes>Build tool dependency</notes>
        <packageUrl regex="true">^pkg:npm/thenify@.*$</packageUrl>
        <cve>CVE-2022-21803</cve>
    </suppress>
    <suppress>
        <notes>Internationalization dependency</notes>
        <packageUrl regex="true">^pkg:npm/y18n@.*$</packageUrl>
        <cve>CVE-2020-7774</cve>
    </suppress>
</suppressions>